{
	"name": "tmdb-nexus-web",
	"version": "2.2.0",
	"private": true,
	"description": "TMDB Nexus App",
	"keywords": [
		"react",
		"next-react-app",
		"typescript"
	],
	"bugs": {
		"url": "https://github.com/timelessco/tmdb-nexus-web/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timelessco/tmdb-nexus-web.git"
	},
	"license": "MIT",
	"author": {
		"name": "Timeless",
		"email": "hello@timeless.co",
		"url": "https://timeless.co/"
	},
	"type": "module",
	"scripts": {
		"build": "turbo run build:next",
		"postbuild": "next-sitemap --config next-sitemap.config.cjs",
		"build:analyze": "cross-env ANALYZE=true pnpm build && pnpm start",
		"build:ci": "cross-env SKIP_ENV_VALIDATION=true next build",
		"build:next": "next build",
		"build:sourcemap": "cross-env SOURCEMAP=true pnpm build && source-map-explorer .next/static/**/*.js && pnpm start",
		"build:start": "pnpm build && pnpm start",
		"clean": "rimraf ./.next ./tsconfig.tsbuildinfo ./node_modules/.cache",
		"clean:all": "turbo clean && pnpm nuke:node_modules && pnpm nuke:turbo",
		"commit": "gacp",
		"contributors:add": "all-contributors add",
		"contributors:generate": "all-contributors generate",
		"dev": "next dev",
		"dev:sourcemap": "cross-env SOURCEMAP=true pnpm dev",
		"format": "turbo format:eslint format:md format:prettier",
		"format:css": "stylelint '**/*.css' --fix --cache --ignore-path .gitignore --color -f verbose",
		"format:eslint": "eslint --max-warnings 0 --ignore-path .gitignore --cache --report-unused-disable-directives --color --fix .",
		"format:md": "markdownlint-cli2 --fix '**/*.md' '.github/**/*.md' '#**/node_modules/**' '#**/CHANGELOG.md'",
		"format:prettier": "prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown .",
		"postinstall": "is-ci || husky install",
		"lint": "turbo run lint:eslint lint:types lint:css lint:md lint:knip lint:package-json lint:spelling lint:prettier --summarize",
		"lint:css": "stylelint '**/*.css' --cache --ignore-path .gitignore --color -f verbose",
		"lint:eslint": "eslint --max-warnings 0 --ignore-path .gitignore --cache --report-unused-disable-directives --ignore-path .gitignore --color .",
		"lint:knip": "knip --production",
		"lint:md": "markdownlint-cli2 '**/*.md' '.github/**/*.md' '#**/node_modules/**' '#**/CHANGELOG.md'",
		"lint:package-json": "npmPkgJsonLint .",
		"lint:prettier": "prettier . --check --cache --cache-location=.prettiercache --ignore-unknown",
		"lint:spelling": "cspell --dot --gitignore --cache --no-progress check \"**/*\"",
		"lint:types": "tsc --noEmit",
		"nuke:node_modules": "rimraf --glob '**/node_modules'",
		"nuke:turbo": "rimraf --glob '**/.turbo'",
		"populate:dictionary": "rimraf project-words.txt && cspell --words-only --unique --gitignore --cache --dot \"**/*\" | sort --ignore-case >> project-words.txt",
		"release": "node release-it/getCommitsSinceLastRelease.js && release-it --ci",
		"release:dryrun": "release-it --dry-run",
		"start": "next start",
		"test": "echo \"no test specified\" && exit 0",
		"update": "taze -I major",
		"update:check": "taze -r major"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"browserslist": {
		"production": [
			">0.3%",
			"not ie 11",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@headlessui/react": "1.7.16",
		"@sentry/nextjs": "7.61.0",
		"ariakit": "2.0.0-next.44",
		"ariakit-react-utils": "0.17.0-next.27",
		"ariakit-utils": "0.17.0-next.27",
		"clsx": "2.0.0",
		"embla-carousel": "8.0.0-rc11",
		"embla-carousel-autoplay": "8.0.0-rc11",
		"embla-carousel-class-names": "8.0.0-rc11",
		"embla-carousel-react": "8.0.0-rc11",
		"fast-blurhash": "1.1.2",
		"framer-motion": "7.8.0",
		"got": "13.0.0",
		"ky": "0.33.3",
		"ky-universal": "0.12.0",
		"next": "13.4.12",
		"next-seo": "6.1.0",
		"qs": "6.11.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "7.45.2",
		"react-player": "^2.12.0",
		"react-remove-scroll": "2.5.6",
		"swr": "2.2.0",
		"url-slug": "4.0.1",
		"zod": "3.21.4",
		"zod-validation-error": "1.3.1"
	},
	"devDependencies": {
		"@babel/plugin-syntax-import-assertions": "7.22.5",
		"@commitlint/cli": "17.6.7",
		"@commitlint/config-conventional": "17.6.7",
		"@ducanh2912/next-pwa": "9.4.0",
		"@ianvs/prettier-plugin-sort-imports": "4.1.0",
		"@next/bundle-analyzer": "13.4.12",
		"@next/env": "13.4.12",
		"@next/eslint-plugin-next": "13.4.12",
		"@octokit/core": "5.0.0",
		"@release-it/conventional-changelog": "7.0.0",
		"@tailwindcss/forms": "0.5.4",
		"@types/node": "20.4.6",
		"@types/qs": "6.9.7",
		"@types/react": "18.2.18",
		"@types/react-dom": "18.2.7",
		"@typescript-eslint/experimental-utils": "5.62.0",
		"all-contributors-cli": "6.26.1",
		"autoprefixer": "10.4.14",
		"babel-loader": "9.1.3",
		"chalk": "5.3.0",
		"cross-env": "7.0.3",
		"cspell": "6.31.2",
		"dedent": "1.5.1",
		"dotenv": "16.3.1",
		"eslint": "8.46.0",
		"eslint-config-canonical": "41.1.7",
		"eslint-config-prettier": "8.10.0",
		"eslint-plugin-jsonc": "2.9.0",
		"eslint-plugin-tailwindcss": "3.13.0",
		"eslint-plugin-toml": "0.5.0",
		"eslint-plugin-yml": "1.8.0",
		"execa": "7.2.0",
		"gacp": "3.0.3",
		"git-remote-origin-url": "4.0.0",
		"git-url-parse": "13.1.0",
		"husky": "8.0.3",
		"is-ci": "3.0.1",
		"jsonc-eslint-parser": "2.3.0",
		"knip": "2.17.3",
		"lint-staged": "13.2.3",
		"markdownlint": "0.29.0",
		"markdownlint-cli2": "0.8.1",
		"next-sitemap": "4.1.8",
		"npm-package-json-lint": "7.0.0",
		"npm-package-json-lint-config-default": "6.0.0",
		"postcss": "8.4.27",
		"prettier": "3.0.1",
		"prettier-plugin-packagejson": "2.4.5",
		"prettier-plugin-tailwindcss": "0.4.1",
		"release-it": "16.1.3",
		"rimraf": "5.0.1",
		"sharp": "0.32.4",
		"source-map-explorer": "2.5.3",
		"stylelint": "15.10.2",
		"stylelint-config-clean-order": "5.0.1",
		"stylelint-config-standard": "34.0.0",
		"tailwindcss": "3.3.3",
		"taze": "0.11.2",
		"tilg": "0.1.1",
		"toml-eslint-parser": "0.6.0",
		"turbo": "1.13.0",
		"typescript": "5.1.6",
		"yaml-eslint-parser": "1.2.2"
	},
	"packageManager": "pnpm@8.6.11",
	"gacp": {
		"add": false,
		"push": false,
		"emoji": "emoji",
		"editor": false
	}
}
